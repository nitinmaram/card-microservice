{
  "name": "cards-microservice",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ./server/bin/www",
    "dev": "nodemon ./server/bin/www",
    "test": "npm run test:coverage npm run test:unit",
    "test:coverage": "nyc",
    "test:unit": "mocha './server/**/*.spec.js'",
    "prisma:initialise:sqllite": "npx prisma init --datasource-provider sqlite",
    "prisma:migration": "npx prisma migrate dev --name init"
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "babel-register": "^6.26.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "swagger-ui-express": "^4.6.0"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "prisma": "^4.9.0",
    "semistandard": "^16.0.1",
    "sinon": "^15.0.1"
  }
}
